using System;
					
public class Program
{
	public static void Main()
	{
		int a = new int();//Вводим размер пирамиды
		a = Convert.ToInt32(Console.ReadLine());//Конвертируем в интовое число 32 битовой системы счисления
		for(int i=1; i<=a; i++){//Рисуем пирамиду по введенному размеру
			for(int j=1;j<=i;j++){//Рисуем пирамиду по введенному размеру
				Console.Write("[*]");//Рисуем пирамиду по введенному размеру
			}
			Console.WriteLine();
		}
		
	}
}
